{
  "name": "attendant-api",
  "version": "0.0.1",
  "description": "",
   
  "author": "Afrilogic Solutions",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "printenv": "echo $NODE_ENV",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate:migration": "ts-node node_modules/.bin/sequelize migration:generate --name",
    "migrate:up": "ts-node node_modules/.bin/sequelize db:migrate --config ./config/config.ts",
    "db:drop": "ts-node node_modules/.bin/sequelize db:drop --url $DATABASE_URL",
    "migrate:undo": "ts-node node_modules/.bin/sequelize db:migrate:undo --url $DATABASE_URL",
    "migrate:undo:all": "ts-node node_modules/.bin/sequelize db:migrate:undo:all --url $DATABASE_URL",
    "db:create": "ts-node node_modules/.bin/sequelize db:create",
    "db:seed-dev": "ts-node node_modules/.bin/sequelize db:seed:all --url $DATABASE_URL",
    "seed:all": "ts-node node_modules/.bin/sequelize db:seed:all --url $DATABASE_URL",
    "seed:undo": "ts-node node_modules/.bin/sequelize db:seed:undo --url $DATABASE_URL",
    "seed:undo:all": "ts-node node_modules/.bin/sequelize db:seed:undo:all --url $DATABASE_URL",
    "db:seed-prod": "ts-node node_modules/.bin/sequelize db:seed:all --url $DATABASE_URL",
    "db:reset": "ts-node db:drop && yarn run db:create && yarn run db:migrate && yarn run db:seed-dev && npm run db:seed-prod",
    "setup": "yarn migrate:up && yarn seed:all",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.16.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.2.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/node": "^7.66.0",
    "@sentry/profiling-node": "^1.2.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/multer": "^1.4.8",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commitizen": "^4.3.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express-session": "^1.17.3",
    "global": "^4.4.0",
    "helmet": "^7.0.0",
    "hubtel-sms-extended": "^1.0.2",
    "ioredis": "^5.3.2",
    "nanoid": "^3.1.30",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.3.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^5.22.3",
    "sequelize-cli": "6.3.0",
    "sequelize-typescript": "^1.1.0",
    "sharp": "^0.32.6",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@automock/jest": "^1.4.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/sequelize": "^4.28.15",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
